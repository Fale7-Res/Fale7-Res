{
  "version": 2,
  "framework": null,
  "functions": {
    "api/*.js": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1" },
    { "src": "/admin", "dest": "/api/admin" },
    { "src": "/(.*)", "dest": "/public/$1" },
    { "src": "/", "dest": "/api/menu" }
  ]
}
